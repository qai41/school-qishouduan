page {
  background-color: #F2F2F2;
}
.bindcar {
  width: 100%;
  padding-top: 30rpx;
  overflow: hidden;
}
.bindcar text {
    font-size: 26rpx;
    color: #777777;
    text-indent: 20rpx;
    line-height: 30rpx;
}
.bindcar .msg {
    color: #DE3535;
}
.bindcar .cantainer {
    width: 100%;
    height: 244rpx;
    background-color: #fff;
    margin: 30rpx 0;
}
.bindcar .cantainer .row {
      height: 80rpx;
      border-bottom: 2rpx solid #CCCCCC;
}
.bindcar .cantainer .row input {
        margin-left: 110rpx;
}
.bindcar .btn-bind {
    background-color: #1A7AFC;
    color: #fff;
    width: 702rpx;
    height: 88rpx;
    line-height: 88rpx;
    margin-top: 80rpx;
}

